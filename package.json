{
  "name": "no-context",
  "version": "1.0.0",
  "description": "",
  "packageManager": "pnpm@10.12.4",
  "scripts": {
    "dev": "concurrently --kill-others \"next dev --turbopack\" \"pnpm run db:sync --accept-data-loss && pnpm run db:studio\"",
    "build": "prisma generate && next build",
    "start": "next start",
    "lint": "eslint . && prettier --check .",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "db:generate": "prisma generate",
    "db:migrate": "prisma migrate dev",
    "db:push": "prisma db push",
    "db:studio": "prisma studio --browser none",
    "db:validate": "prisma validate",
    "db:sync": "pnpm db:validate && pnpm db:generate && pnpm db:push",
    "db:sync:force": "pnpm db:validate && pnpm db:generate && prisma db push --accept-data-loss",
    "db:seed": "pnpm db:sync && prisma db seed",
    "up": "docker compose -f dev/docker-compose.yml up -d --wait",
    "down": "docker compose -f dev/docker-compose.yml down",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci --coverage --maxWorkers=2",
    "setup": "bash setup.sh",
    "postinstall": "prisma generate",
    "prepare": "husky"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.77.0",
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/sortable": "^10.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@prisma/adapter-pg": "^7.4.0",
    "@prisma/client": "^7.4.0",
    "@prisma/client-runtime-utils": "^7.4.0",
    "@react-email/components": "^1.0.8",
    "@react-email/render": "^2.0.4",
    "@slack/web-api": "^7.14.1",
    "@tailwindcss/postcss": "^4.2.0",
    "@types/node": "^25.3.0",
    "@types/react": "^19.2.14",
    "@types/react-dom": "^19.2.3",
    "@upstash/qstash": "^2.9.0",
    "@upstash/redis": "^1.36.2",
    "@vercel/analytics": "^1.6.1",
    "@vercel/blob": "^2.2.0",
    "bcryptjs": "^3.0.3",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "dotenv": "^17.3.1",
    "lucide-react": "^0.574.0",
    "motion": "^12.34.2",
    "next": "^16.1.6",
    "next-auth": "5.0.0-beta.30",
    "nodemailer": "^8.0.1",
    "openai": "^6.22.0",
    "postcss": "^8.5.6",
    "prisma": "^7.4.0",
    "radix-ui": "^1.4.3",
    "react": "^19.2.4",
    "react-dom": "^19.2.4",
    "resend": "^6.9.2",
    "sharp": "^0.34.5",
    "sonner": "^2.0.7",
    "stripe": "^20.3.1",
    "tailwind-merge": "^3.5.0",
    "tailwindcss": "^4.2.0",
    "typescript": "^5.9.3",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.2",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^30.0.0",
    "@types/nodemailer": "^7.0.10",
    "concurrently": "^9.2.1",
    "eslint": "^9.39.2",
    "eslint-config-next": "^16.1.6",
    "husky": "^9.1.7",
    "jest": "^30.2.0",
    "jest-environment-jsdom": "^30.2.0",
    "jest-mock-extended": "^4.0.0",
    "lint-staged": "^16.2.7",
    "node-mocks-http": "^1.17.2",
    "prettier": "^3.8.1",
    "prettier-plugin-tailwindcss": "^0.7.2",
    "puppeteer": "^24.37.5",
    "shadcn": "^3.8.5",
    "ts-jest": "^29.4.6",
    "tsx": "^4.21.0",
    "tw-animate-css": "^1.4.0"
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx,css,json,md}": "prettier --write"
  },
  "prisma": {
    "seed": "npx tsx prisma/seed.ts"
  }
}
